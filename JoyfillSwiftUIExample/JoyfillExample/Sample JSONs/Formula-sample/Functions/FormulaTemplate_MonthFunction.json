    {
      "_id": "691acd93f6346774b0b9d077",
      "desc": "formular-4",
      "scope": "private",
      "type": "calc",
      "expression": "if(month(sampleDate) == 1, \"January\", if(month(sampleDate) == 2, \"February\", if(month(sampleDate) == 3, \"March\", if(month(sampleDate) == 4, \"April\", if(month(sampleDate) == 5, \"May\", if(month(sampleDate) == 6, \"June\", if(month(sampleDate) == 7, \"July\", if(month(sampleDate) == 8, \"August\", if(month(sampleDate) == 9, \"September\", if(month(sampleDate) == 10, \"October\", if(month(sampleDate) == 11, \"November\", \"December\"))))))))))))"
    },
    {
      "_id": "691acd93f033ed54812dc643",
      "desc": "formular-5",
      "scope": "private",
      "type": "calc",
      "expression": "if(month(now()) >= 3 && month(now()) <= 5, \"Spring\", if(month(now()) >= 6 && month(now()) <= 8, \"Summer\", if(month(now()) >= 9 && month(now()) <= 11, \"Fall\", \"Winter\")))"
    },
    {
      "_id": "691acd93f033ed54812dc644",
      "desc": "formular-6",
      "scope": "private",
      "type": "calc",
      "expression": "\"Q\" + ceil(month(sampleDate) / 3) + \" \" + year(sampleDate)"
    },
    {
      "_id": "691acd93f033ed54812dc645",
      "desc": "formular-7",
      "scope": "private",
      "type": "calc",
      "expression": "if(month(eventDate) == month(now()) && year(eventDate) == year(now()), \"This month\", \"Different month\")"
    }
  ]
