    {
      "_id": "691acd93f033ed54812dc636",
      "desc": "formular-5",
      "scope": "private",
      "type": "calc",
      "expression": "if(score >= max([score1, score2, score3, score4, score5]) - 10, \"A\", if(score >= max([score1, score2, score3, score4, score5]) - 20, \"B\", if(score >= max([score1, score2, score3, score4, score5]) - 30, \"C\", if(score >= max([score1, score2, score3, score4, score5]) - 40, \"D\", \"F\"))))"
    },
    {
      "_id": "691acd93f033ed54812dc637",
      "desc": "formular-6",
      "scope": "private",
      "type": "calc",
      "expression": "max(concat([baseValue], [price1, price2, price3]))"
    }
