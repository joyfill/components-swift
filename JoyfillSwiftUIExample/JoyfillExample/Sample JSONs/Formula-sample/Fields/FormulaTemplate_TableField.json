  "name": "New Doc",
  "formulas": [
    {
      "_id": "displayCellValue",
      "desc": "Display table1 row 1 text column cell value.",
      "type": "calc",
      "scope": "private",
      "expression": "table1.0.text1"
    },
    {
      "_id": "countEmptyDateRows",
      "desc": "Count rows where date1 is empty",
      "type": "calc",
      "scope": "private",
      "expression": "length(filter(table1, (row) -> if(empty(row.date1), true, false)))"
    },
    {
      "_id": "multiCriteriaCount",
      "desc": "Count rows where text1 contains 'b', multiSelect1 includes Option 1 D1 and Option 2 D1, and number1 is 200 or 22",
      "type": "calc",
      "scope": "private",
      "expression": "length(\n  filter(table1, (row) -> \n    and(\n      if(contains(lower(row.text1), \"b\"), true, false),\n      and(\n        some(row.multiSelect1, (option) -> option == \"Option 1 D1\"),\n        some(row.multiSelect1, (option) -> option == \"Option 2 D1\")\n      ),\n      or(row.number1 == 200, row.number1 == 22)\n    )\n  )\n)"
    },
    {
      "_id": "combineText1Values",
      "desc": "Reduce all text1 column values into a single string",
      "type": "calc",
      "scope": "private",
      "expression": "reduce(table1, (acc, row) -> concat(acc, row.text1), \"\")"
    },
    {
      "_id": "countYesDropdown",
      "desc": "Count number of rows where dropdown1 == 'Yes D1'",
      "type": "calc",
      "scope": "private",
      "expression": "length(filter(table1, (row) -> row.dropdown1 == \"Yes D1\"))"
    }
  ],
