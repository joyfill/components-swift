  "formulas": [
    {
      "_id": "extractYear",
      "desc": "Extract year from Date 1",
      "type": "calc",
      "scope": "global",
      "expression": "year(date1)"
    },
    {
      "_id": "extractMonth",
      "desc": "Extract month from Date 1",
      "type": "calc",
