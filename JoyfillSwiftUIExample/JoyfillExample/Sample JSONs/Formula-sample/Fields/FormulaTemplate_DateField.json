    },
    {
      "_id": "extractDay",
      "desc": "Extract day from Date 1",
      "type": "calc",
      "scope": "global",
      "expression": "day(date1)"
    },
    {
      "_id": "add5Days",
      "desc": "Add 5 days to Date 1",
      "type": "calc",
      "scope": "global",
      "expression": "dateAdd(date1, 5, \"days\")"
    },
    {
      "_id": "subtract2Weeks",
      "desc": "Subtract 2 weeks from Date 1",
      "type": "calc",
      "scope": "global",
      "expression": "dateSubtract(date1, 2, \"weeks\")"
    },
    {
      "_id": "daysBetweenNow",
      "desc": "Days between now and Date 1",
      "type": "calc",
      "scope": "global",
      "expression": "(now() - date1) / (1000 * 60 * 60 * 24)"
    },
    {
      "_id": "isFutureDate",
      "desc": "Check if Date 1 is in the future",
      "type": "calc",
      "scope": "global",
      "expression": "date1 > now()"
    },
    {
      "_id": "pastOrUpcoming",
      "desc": "Return 'Past' or 'Upcoming' based on Date 1",
      "type": "calc",
      "scope": "global",
      "expression": "if(date1 < now(), \"Past\", \"Upcoming\")"
    }
