  "type": "document",
  "formulas": [
    {
      "_id": "checkForUrgentText",
      "desc": "Check if textarea contains 'urgent'",
      "type": "calc",
      "scope": "private",
      "expression": "contains(lower(textarea1), \"urgent\")"
    },
    {
      "_id": "uppercaseAll",
      "desc": "Uppercase all textarea content",
      "type": "calc",
      "scope": "private",
      "expression": "upper(textarea1)"
    },
    {
      "_id": "lowercaseAll",
      "desc": "Lowercase all textarea content",
      "type": "calc",
      "scope": "private",
      "expression": "lower(textarea1)"
    },
    {
      "_id": "appendNoteAtEnd",
      "desc": "Append a note to the end",
      "type": "calc",
      "scope": "private",
      "expression": "concat(textarea1, \"\\n-- Reviewed by Admin --\")"
    },
    {
      "_id": "checkForEmpty",
      "desc": "Check if textarea is empty",
      "type": "calc",
      "scope": "private",
      "expression": "empty(textarea1)"
    },
    {
      "_id": "checkForErrorText",
      "desc": "Status message if contains 'error'",
      "type": "calc",
      "scope": "private",
      "expression": "if(contains(lower(textarea1), \"error\"), \"Error Detected\", \"All Clear\")"
    },
    {
      "_id": "prefixText",
      "desc": "Prefix with 'User Notes:'",
      "type": "calc",
      "scope": "private",
      "expression": "concat(\"User Notes:\\n\", textarea1)"
    },
    {
      "_id": "countCharactersInText",
      "desc": "Count characters in textarea",
      "type": "calc",
      "scope": "private",
      "expression": "length(textarea1)"
    }
  ]
